{
  "manifest_version": 2,
  "name": "Inspect SVG",
  "description": "Try to find and show SVG symbols used on a page.",
  "homepage_url": "https://github.com/fvsch/devtools-svg",
  "author": "Florens Verschelde",
  "version": "1.0",
  "icons": {
    "64": "img/lion-64.png",
    "128": "img/lion-128.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": ["<all_urls>"],
  "devtools_page": "devtools/devtools.html"
}
